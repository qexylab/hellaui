{
  "name": "hellaui",
  "version": "1.0.4",
  "description": "Fully accessible React UI components for your website that use Tailwind CSS",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "main": "./dist/hellaui.cjs.js",
  "module": "./dist/hellaui.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/hellaui.cjs.js",
      "import": "./dist/hellaui.es.js"
    }
  },
  "imports": {
    "@/src/*": "./src/*"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build && ttsc --emitDeclarationOnly",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "prettier": "prettier --write ."
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "repository": "https://github.com/qexylab/hellaui",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "dependencies": {
    "@babel/core": "^7.18.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@types/jest": "^29.0.0",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "jest": "^29.0.1",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.1.8",
    "ttypescript": "^1.5.13",
    "typescript": "4.7.4",
    "vite": "^3.0.9",
    "typescript-transform-paths": "^3.3.1",
    "sass": "^1.54.5"
  },
  "bugs": {
    "url": "https://github.com/qexylab/hellaui/issues"
  }
}
